# Креды для авторизации на сайтах
LOGIN=your_login_here
PASSWORD=your_password_here

# false = браузер открывается с окном (удобно для отладки)
# true = браузер работает в фоне без окна
HEADLESS=false

# Задержка между действиями в миллисекундах
# 0 = без задержки, 500-1000 = удобно для наблюдения за тестом
SLOW_MO=0

# Список известных адресов крипто-кошельков через запятую
# Используется когда провайдер неизвестен — тест проверяет совпадение с одним из этих адресов
# Пример: KNOWN_WALLETS=TXyz123abc,TXyz456def
KNOWN_WALLETS=